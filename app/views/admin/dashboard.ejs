<!-- Datepicker -->
<link
	rel="stylesheet"
	href="<%- baseUrl %>admin/vendors/datepicker/jquery-ui.css"
/>
<script src="<%- baseUrl %>admin/vendors/datepicker/jquery-ui.js"></script>

<script>
	function change() {
		var min, max;
		min = $('#min').datepicker('getDate');
		max = $('#max').datepicker('getDate');
		if (min != undefined && max != undefined && min != null && max != null) {
			if (min > max) {
				Swal.fire('From date should less than To date');
				return false;
			}
			return true;
		}
	}
	$(document).ready(function () {
		$('#min').datepicker({
			//minDate: new Date(),
			changeMonth: true,
			changeYear: true,
			dateFormat: 'yy-mm-dd',
			maxDate: 0,
		});

		$('#max').datepicker({
			//minDate: new Date(),
			changeMonth: true,
			changeYear: true,
			dateFormat: 'yy-mm-dd',
			maxDate: 0,
		});
		$('#isp_min').datepicker({
			//minDate: new Date(),
			changeMonth: true,
			changeYear: true,
			dateFormat: 'yy-mm-dd',
			maxDate: 0,
		});

		$('#isp_max').datepicker({
			//minDate: new Date(),
			changeMonth: true,
			changeYear: true,
			dateFormat: 'yy-mm-dd',
			maxDate: 0,
		});

		$(function () {
			$("form[name='paymentFilter']").validate({
				ignore: [],
				debug: false,
				rules: {
					name: {
						required: true,
						lettersonly: true,
						maxlength: 40,
						normalizer: function (value) {
							return $.trim(value);
						},
					},
					max: {
						required: true,
						greaterThan: '#min',
					},
					min: {
						required: true,
					},
				},
				messages: {
					max: {
						required: 'Please select To date.',
						greaterThan: 'From date should less than To date.',
					},
					min: {
						required: 'Please select From date.',
						//	greaterThan:"Must be greater than {0}."
					},
				},

				submitHandler: function (form) {
					$(':submit').attr('disabled', true);
					form.submit();
				},
			});
		});
		$(function () {
			$("form[name='ispFilter']").validate({
				ignore: [],
				debug: false,
				rules: {
					name: {
						required: true,
						lettersonly: true,
						maxlength: 40,
						normalizer: function (value) {
							return $.trim(value);
						},
					},
					isp_max: {
						required: true,
						greaterThan: '#isp_min',
					},
					isp_min: {
						required: true,
					},
				},
				messages: {
					isp_max: {
						required: 'Please select To date.',
						greaterThan: 'From date should less than To date.',
					},
					isp_min: {
						required: 'Please select From date.',
						//	greaterThan:"Must be greater than {0}."
					},
				},

				submitHandler: function (form) {
					$(':submit').attr('disabled', true);
					form.submit();
				},
			});
		});
	});
</script>
