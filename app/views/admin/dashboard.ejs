	function drawChart() {
		var userData = [];
		var paymentReports = $('#paymentReports').html();
		paymentReports = JSON.parse(paymentReports);
		for (var i = 0; i < paymentReports.length; i++) {
			var temp = [];
			temp.push(paymentReports[i].key); //userData[i].MonthValue
