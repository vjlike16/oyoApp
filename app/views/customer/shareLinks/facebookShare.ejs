      <script src="<%- baseUrl %>admin/vendors/jquery/dist/jquery.min.js"></script>
 
   </head>
   <body style="color: white !important;">
      <script>
         var url_string = location.href;
         var url = new URL(url_string);
         var c = url.searchParams.get("verifyCode");
		 setTimeout(function(){ 
			window.location.href = `<%- baseUrl %>inviteCodeRedirection/${c}`; 
		 }, 1000);
        
         
      </script>
   </body>
