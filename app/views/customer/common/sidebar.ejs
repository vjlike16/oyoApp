	<script>
		$(document).ready(function () {
			$('.renewPlan').click(function () {
				// $(".common-notification-hideShow").toggle();
				Swal.fire('Please renew your subscription.');
			});
		});
	</script>
	<script>
		$(document).ready(function () {
			$('.mobile_menu_btn').click(function () {
				$('.mobile_menu').addClass('menu_open');
			});

			$('.menu_close').click(function () {
				$('.mobile_menu').removeClass('menu_open');
			});
		});

		$.ajax({
			type: 'GET',
			url: '<%- baseUrl %>unReadPaymentReminder',
			success: function (data) {
				if (!data.response) {
					$('.payment_indicator').css('display', 'block');
				}
			},
		});
	</script>
	<script>
		$(document).on('click', '.service-link', function () {
			console.log('Done');
			var finalTimeZone = moment.tz.guess();
			console.log('finalTimeZone', finalTimeZone);
			$.ajax({
				type: 'POST',
				url: `<%- baseUrl %>getTimeZone`,
				data: {
					timezone: function () {
						return finalTimeZone;
					},
				},
				success: function (data) {
					var data = data.data;
					console.log('data', data);
				},
			});
		});
	</script>